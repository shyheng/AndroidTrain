<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ui.main.BlankFragment1">

    <TextView
        android:id="@+id/tv_main"
        android:textSize="50dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:textColor="@color/black"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="第一个页面" />

    <Button
        android:id="@+id/main_g"
        app:layout_constraintBottom_toTopOf="@id/tv_main"
        app:layout_constraintRight_toRightOf="@id/tv_main"
        app:layout_constraintLeft_toLeftOf="@id/tv_main"
        android:layout_width="200dp"
        android:background="@color/teal_200"
        android:text="跳转到引导页"
        android:layout_height="wrap_content"/>

    <Button
        android:id="@+id/main_minute"
        app:layout_constraintRight_toRightOf="@id/tv_main"
        app:layout_constraintLeft_toLeftOf="@id/tv_main"
        app:layout_constraintTop_toBottomOf="@id/tv_main"
        android:layout_width="300dp"
        android:text="点击跳转"
        android:layout_height="wrap_content"/>

</androidx.constraintlayout.widget.ConstraintLayout>